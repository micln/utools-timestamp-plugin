{
	"preload": "preload.js",
	"logo": "logo.png",
	"features": [
		{
			"code": "time",
			"explain": "输入时间戳或日期，返回转换后的结果",
			"cmds": [
				"t",
				"time",
				"timestamp",
				"now",
				{
					"type": "regex",
					"label": "时间戳转换",
					"match": "/(^\\d{4}.\\d{0,2}.\\d{0,2})|(^\\d{8,13}$)/",
					"minLength": 8,
					"maxLength": 13
				}
			]
		}
	]
}